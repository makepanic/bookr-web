
{
    "env": {
        "browser": true,
        "node": false,
        "amd": false
    },

    "rules": {
        "no-alert": 2,
        "no-caller": 2,
        "no-bitwise": 0,
        "no-console": 0,
        "no-comma-dangle": 2,
        "no-debugger": 2,
        "no-div-regex": 0,
        "no-dupe-keys": 2,
        "no-else-return": 0,
        "no-empty": 2,
        "no-empty-class": 2,
        "no-eq-null": 0,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-floating-decimal": 0,
        "no-implied-eval": 2,
        "no-with": 2,
        "no-fallthrough": 2,
        "no-unreachable": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-octal": 2,
        "no-obj-calls": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-native-reassign": 2,
        "no-plusplus": 0,
        "no-delete-var": 2,
        "no-return-assign": 2,
        "no-new-array": 2,
        "no-new-object": 2,
        "no-label-var": 2,
        "no-ternary": 0,
        "no-self-compare": 0,
        "no-spaced-func": 2,
        "no-sync": 0,
        "no-underscore-dangle": 0,
        "no-loop-func": 2,
        "no-empty-label": 2,
        "no-unused-vars": 2,
        "no-script-url": 2,
        "no-proto": 2,
        "no-iterator": 2,
        "no-mixed-requires": [0, false],
        "no-wrap-func": 2,
        "no-shadow": 2,
        "no-use-before-define": 2,

        "brace-style": 0,
        "block-scoped-var": 0,
        "camelcase": 2,
        "curly": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "new-parens": 2,
        "guard-for-in": 0,
        "radix": 0,
        "new-cap": 2,
        "one-var": 0,
        "quote-props": 0,
        "semi": 2,
        "use-isnan": 2,
        "quotes": [2, "single"],
        "max-depth": [0, 4],
        "max-params": [0, 3],
        "max-statements": [0, 20],
        "regex-spaces": 2,
        "complexity": [0, 22],
        "wrap-iife": 0,
        "wrap-regex": 0,
        "no-multi-str": 2,
        "consistent-this": [0, "that"],
        "unnecessary-strict": 2,
        "max-len": [0, 80, 4],
        "strict": 0
    }
}
